<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="lab_1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:lab_1"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Calculator">
        <ShellContent Title="Calculator"
                      ContentTemplate="{DataTemplate local:MainPage}"/>
    </FlyoutItem>

    <FlyoutItem Title="ProgressBar">
        <ShellContent Title="ProgressBar"
                      ContentTemplate="{DataTemplate local:ProgressBar}"/>
    </FlyoutItem>

    <FlyoutItem Title="SQLite">
        <ShellContent Title="SQLite"
                      ContentTemplate="{DataTemplate local:lab_3.SQLiteContentPage}"/>
    </FlyoutItem>
    
    

</Shell>
